<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>Raz App</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link id="bs-css" href="css/bootstrap-spacelab.css" rel="stylesheet">
    <link href="~/Content/css/social-buttons.css" rel="stylesheet" />
    <link href="~/Content/css/General.css" rel="stylesheet" />
    @Styles.Render("~/bundles/bootstrap/css")
    <style type="text/css">
        body {
            padding-bottom: 40px;
        }

        .sidebar-nav {
            padding: 9px 0;
        }
    </style>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Scripts/spin.js"></script>

</head>
<body>

    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="/Home/Index">
                    <span>My Web App</span>
                </a>
              
                <!-- user dropdown starts -->

                @Html.Partial("_LoginPartial")

                <!-- user dropdown ends -->
            </div>
        </div>
    </div>


    <div class="container-fluid">
        <div class="row-fluid">
            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
            </noscript>
            <div id="content" class="row-fluid">
                <div class="span6 centre">
                    <img src="~/Content/images/replacement%20people.jpg" />
                </div>
              <div class="span6 pull-right">
                   @RenderBody()
              </div>
               

               
            </div>
           
        </div>
     

        <hr>




    </div>



    <script src="~/Content/Bootstrap/js/jquery-ui-1.8.21.custom.min.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-alert.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-button.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-carousel.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-collapse.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-dropdown.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-modal.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-popover.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-scrollspy.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-tab.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-toggle.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-tooltip.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-tour.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-transition.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap-typeahead.js"></script>
    <script src="~/Content/Bootstrap/js/charisma.js"></script>
    <script src="~/Content/Bootstrap/js/excanvas.js"></script>
    <script src="~/Content/Bootstrap/js/fullcalendar.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.autogrow-textarea.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.chosen.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.cleditor.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.colorbox.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.cookie.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.elfinder.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.flot.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.flot.pie.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.flot.resize.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.flot.stack.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.history.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.iphone.toggle.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.noty.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.raty.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.uniform.min.js"></script>
    <script src="~/Content/Bootstrap/js/jquery.uploadify-3.1.min.js"></script>


    @*@Scripts.Render("~/bundles/bootstrap/js")*@
    @RenderSection("scripts", required: false)

    
<button id="message-info" style="display:none;" class="btn btn-primary noty" data-noty-options="{&quot;text&quot;:&quot;MESSAGE&quot;,&quot;layout&quot;:&quot;top&quot;,&quot;type&quot;:&quot;information&quot;}"><i class="icon-bell icon-white"></i>info </button>
<button id="message-error" style="display:none;" class="btn btn-primary noty" data-noty-options="{&quot;text&quot;:&quot;MESSAGE&quot;,&quot;layout&quot;:&quot;top&quot;,&quot;type&quot;:&quot;error&quot;}"><i class="icon-bell icon-white"></i> error</button>
<button id="message-success" style="display:none;" class="btn btn-primary noty" data-noty-options="{&quot;text&quot;:&quot;MESSAGE&quot;,&quot;layout&quot;:&quot;top&quot;,&quot;type&quot;:&quot;success&quot;}"><i class="icon-bell icon-white"></i>success</button>

</body>
</html>
